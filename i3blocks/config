[users]
command=users
interval=10

[volume]
command=~/.config/i3blocks/scripts/volume.sh
label=♪ 
interval=60
signal=10

[disk-/]
command=~/.config/i3blocks/scripts/disk.sh
label=/ 
DIR=/
interval=30

[disk /home]
command=~/.config/i3blocks/scripts/disk.sh
label=/home 
DIR=/home
interval=30

[disk /storage]
command=~/.config/i3blocks/scripts/disk.sh
label=/storage 
DIR=/storage
interval=30

[weather]
command=~/.config/i3blocks/scripts/weather.sh
interval=60

[iface eth0]
command=~/.config/i3blocks/scripts/iface.sh
label=eth0: 
IFACE=eth0
interval=10

[iface wlp2s0]
command=~/.config/i3blocks/scripts/iface.sh
label=wifi: 
IFACE=wlp2s0
interval=10

[iface wg0]
command=~/.config/i3blocks/scripts/iface.sh
label=wg0: 
IFACE=wg0
interval=10

[temperature]
command=~/.config/i3blocks/scripts/temperature.sh
interval=10
T_WARN=70
T_CRIT=90

[batterybar]
command=~/.config/i3blocks/scripts/battery.sh
label=bat:
interval=5
markup=pango
min_width=bat: ■■■■■

# Update time every 5 seconds
[time]
command=date +"[%d %b] %T"
interval=5
